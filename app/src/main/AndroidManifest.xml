<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.ivan.leo.informationbar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_burned"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".RSSactivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RssItemDisplayer" android:label="Rss News">
            <intent-filter>
                <action android:name="ru.ivanov.leo.displayRssItem" />
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>
<uses-permission android:name="android.permission.INTERNET" />

</manifest>